{
  "manifest_version": 2,

  "name": "Never Normal: Trump",
  "description": "Reminds folks of our leader's true nature",
  "version":"0.0.0.3",
  "browser_action": {
    "default_icon": "trump_icon.png"
  },
  "author": "Matty Z",
  "short_name": "Never Normal: Trump",
  "permissions": [
  "https://www.google-analytics.com/",
  "storage"
],
  "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}